<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DashboardActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="177dp"
        android:layout_height="189dp"
        android:background="@drawable/addaccount"
        android:gravity="bottom|center"
        android:textColor="@android:color/white"
        android:textSize="19sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="95dp" />
]
    <Button
        android:id="@+id/button7"
        android:layout_width="177dp"
        android:layout_height="189dp"
        android:background="@drawable/manageaccount"
        android:gravity="bottom|center"
        android:textColor="@android:color/white"
        android:textSize="19sp"
        tools:layout_editor_absoluteX="218dp"
        tools:layout_editor_absoluteY="95dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="177dp"
        android:layout_height="189dp"
        android:background="@drawable/searchemployee"
        android:gravity="bottom|center"
        android:textColor="@android:color/white"
        android:textSize="19sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="305dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="177dp"
        android:layout_height="189dp"
        android:background="@drawable/searchjob"
        android:gravity="bottom|center"
        android:textColor="@android:color/white"
        android:textSize="19sp"
        tools:layout_editor_absoluteX="218dp"
        tools:layout_editor_absoluteY="305dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="177dp"
        android:layout_height="189dp"
        android:background="@drawable/logout"
        android:gravity="bottom|center"
        android:textColor="@android:color/white"
        android:textSize="55sp"
        tools:layout_editor_absoluteX="218dp"
        tools:layout_editor_absoluteY="513dp" />

    <Button
        android:id="@+id/button11"
        android:layout_width="177dp"
        android:layout_height="189dp"
        android:background="@drawable/addjob"
        android:gravity="bottom|center"
        android:textColor="@android:color/white"
        android:textSize="19sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="513dp" />

    <android.support.constraint.Barrier
        android:id="@+id/barrier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="446dp"
        android:layout_height="70dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginBottom="84dp"
        android:background="@android:color/white"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.604"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/topbar" />

</android.support.constraint.ConstraintLayout>